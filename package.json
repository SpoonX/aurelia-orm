  "dependencies": {
    "aurelia-api": "^3.0.0",
    "aurelia-binding": "^1.0.0",
    "aurelia-dependency-injection": "^1.0.0",
    "aurelia-logging": "^1.0.0",
    "aurelia-metadata": "^1.0.0",
    "aurelia-templating": "^1.0.0",
    "aurelia-validation": "^0.13.0",
    "get-prop": "^0.0.10",
    "typer": "^1.1.0"
  },
  "devDependencies": {
    "spoonx-tools": "^1.1.1"
  },
